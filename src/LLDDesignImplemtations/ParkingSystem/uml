                    ┌────────────────────┐
                    │     Vehicle        │
                    ├────────────────────┤
                    │- vehicleNumber     │
                    │- type: VehicleType │
                    └────────────────────┘
                            ▲
            ┌───────────────┼───────────────┐
            │                               │
 ┌─────────────────────┐         ┌────────────────────┐
 │   TwoWheeler        │         │   FourWheeler      │
 └─────────────────────┘         └────────────────────┘


 ┌────────────────────┐
 │   ParkingSpot      │
 ├────────────────────┤
 │- id                │
 │- spotType          │
 │- isAvailable       │
 │- parkedVehicle     │
 └────────────────────┘


 ┌──────────────────────┐
 │   Ticket             │
 ├──────────────────────┤
 │- ticketId            │
 │- vehicle             │
 │- spot                │
 │- entryTime, exitTime │
 └──────────────────────┘


 ┌────────────────────────────┐
 │   ParkingStrategy (interface) │
 ├────────────────────────────┤
 │+ findParkingSpot()         │
 └────────────────────────────┘
              ▲
              │ implements
 ┌──────────────────────────┐
 │ NearestSpotStrategy      │
 └──────────────────────────┘


 ┌──────────────────────────────────┐
 │   ParkingSpotManager (abstract)  │
 ├──────────────────────────────────┤
 │- spots : List<ParkingSpot>       │
 │- strategy : ParkingStrategy      │
 │+ findParkingSpot()               │
 │+ addSpot()                       │
 │+ removeSpot()                    │
 └──────────────────────────────────┘
              ▲
      ┌───────┼────────┐
      │                │
┌──────────────────────────────┐   ┌──────────────────────────────┐
│ TwoWheelerSpotManager        │   │ FourWheelerSpotManager       │
└──────────────────────────────┘   └──────────────────────────────┘


 ┌────────────────────────────┐
 │ ParkingSpotManagerFactory  │
 ├────────────────────────────┤
 │+ getManager(VehicleType)   │
 └────────────────────────────┘


 ┌──────────────────────┐         ┌──────────────────────┐
 │   EntranceGate       │         │     ExitGate         │
 ├──────────────────────┤         ├──────────────────────┤
 │+ generateTicket()    │         │+ processExit()       │
 └──────────────────────┘         └──────────────────────┘


 ┌──────────────────────┐
 │   ParkingLot         │
 ├──────────────────────┤
 │- twoWheelerManager   │
 │- fourWheelerManager  │
 │+ parkVehicle()       │
 │+ vacateSpot()        │
 └──────────────────────┘
